# API Secret (shared between cockpit and MCP server)
COSHIP_API_SECRET=

# Cockpit URL (for branded OAuth authorization UI)
COSHIP_COCKPIT_URL=http://localhost:3000

# Skills Configuration
COSHIP_SKILLS_DIR=skills
